# Development environment configuration
NODE_ENV=development
LOG_LEVEL=debug
PORT=3000

# Database
DB_HOST=localhost
DB_PORT=5432
DB_NAME=annotation_dev
DB_USER=postgres
DB_PASSWORD=dev_password

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=dev_redis_password

# Security (development only - generate secure values for production)
JWT_SECRET=dev_jwt_secret_change_me_in_production
SESSION_SECRET=dev_session_secret_change_me_in_production

# CORS
CORS_ORIGIN=http://localhost:3000,http://localhost:3001

# Features
DEBUG_MODE=true
HOT_RELOAD=true
MOCK_EXTERNAL_APIS=true

# Logging
LOG_FILE=false
CONSOLE_LOG=true

# Performance
COMPRESSION_ENABLED=false
CACHE_TTL=60

# Development tools
ENABLE_DOCS=true
ENABLE_PLAYGROUND=true