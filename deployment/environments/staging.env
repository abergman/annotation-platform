# Staging environment configuration
NODE_ENV=staging
LOG_LEVEL=info
PORT=3000

# Database
DB_HOST=staging-db.internal
DB_PORT=5432
DB_NAME=annotation_staging
DB_USER=staging_user
# DB_PASSWORD should be set via secrets management

# Redis
REDIS_HOST=staging-redis.internal
REDIS_PORT=6379
# REDIS_PASSWORD should be set via secrets management

# Security (set via secrets management)
# JWT_SECRET=
# SESSION_SECRET=

# CORS
CORS_ORIGIN=https://staging.annotation.com

# Features
DEBUG_MODE=false
HOT_RELOAD=false
MOCK_EXTERNAL_APIS=false

# Logging
LOG_FILE=true
CONSOLE_LOG=true

# Performance
COMPRESSION_ENABLED=true
CACHE_TTL=300

# Monitoring
ENABLE_METRICS=true
HEALTH_CHECK_ENABLED=true

# Rate limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100